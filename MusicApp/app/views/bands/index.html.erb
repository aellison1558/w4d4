<h1>Bands</h1>

<%= link_to "Add Band", new_band_url %>
<%= link_to "Add Album", new_album_url %>

<p>
  <table>
    <tr>
      <th>Band</th>

    </tr>
    <% @bands.each do |band| %>
      <tr>
        <td><%= link_to "#{band.name}", band_url(band) %></td>
        <td><%= link_to "albums", band_albums_url(band) %></td>
      </tr>

    <% end %>
  </table>
</p>
